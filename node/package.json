{
  "name": "node",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start.dev": "nodemon -L"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.0.0",
    "express": "^4.17.1",
    "http-errors": "~1.6.3",
    "moment": "^2.30.1",
    "morgan": "~1.9.1",
    "pg": "^8.11.5",
    "serve-favicon": "2.5.0",
    "twilio": "^4.5.0",
    "ejs": "^3.1.10",
    "express-session": "^1.18.0",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2"
  },
  "devDependencies": {
    "nodemon": "3.1.0"
  },
  "nodemonConfig": {
    "ext": "js,json,ts,html,ejs,css,",
    "watch": [
      "./*"
    ],
    "ignore": [
      "node_modules/**/node_modules"
    ],
